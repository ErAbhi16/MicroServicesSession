{
  "spring": {
    "application": {
      "name": "CartService"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka/",
      "validateCertificates": false
    },
    "instance": {
      "instanceId": "CartService",
      "hostname": "localhost",
      "port": "7124"
    }
  },
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
